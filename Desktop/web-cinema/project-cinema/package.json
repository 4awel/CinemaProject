{
  "name": "project-cinema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "dev:full": "concurrently -n CLIENT,SERVER -c cyan,magenta \"npm run dev\" \"npm run dev:server\"",
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "tsc": "tsc",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/babel-preset-jsx": "^1.4.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "env": "^0.0.2",
    "express": "^5.1.0",
    "gsap": "^3.12.7",
    "i18n": "^0.15.1",
    "jarallax": "^2.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.3",
    "multer": "^2.0.2",
    "node-ts": "^6.1.3",
    "vue": "^3.5.13",
    "vue-i18n": "^12.0.0-alpha.2",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.18",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^7.0.2",
    "sass-embedded": "^1.86.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.2"
  }
}
